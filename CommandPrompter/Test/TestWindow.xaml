<Window x:Class="CommandPrompter.Test.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommandPrompter.Test"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="800"
        DataContext="{x:Static local:TestWindowViewModel.Instance}">
    <Grid>
        <ListView ItemsSource="{Binding Users}">
            <ListView.View>
                <GridView>
                    <GridView.Columns>
                        <GridViewColumn Header="Username" DisplayMemberBinding="{Binding Username}"/>
                        <GridViewColumn Header="Email" DisplayMemberBinding="{Binding Email}"/>
                        <GridViewColumn Header="Contributes" DisplayMemberBinding="{Binding Contributes}"/>
                        <GridViewColumn Header="RegisterTime" DisplayMemberBinding="{Binding RegisterTime}"/>
                    </GridView.Columns>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
